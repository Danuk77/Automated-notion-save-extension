{
    "name": "Automated Notion Save",
    "version": "1.0.0",
    "description": "Application to save pages directly to Notion",
    "manifest_version": 3,
    "author": "Danuk Sanujaya Udagama",
    "action": {
        "default_popup": "index.html",
        "default_title": "Save to Notion"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "web_accessible_resources": [
        {
            "resources":[
                "Assets/Logo.png",
                "Assets/Save.png"
            ],
            "matches":["https://*/*"]
        }
    ]
}